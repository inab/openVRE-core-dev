{
  "_id": "biobb_disgenet",
  "name": "Get a PPI network using GUILD algorithms and DisGenet results",
  "title": "Get a PPI network using GUILD algorithms and DisGenet results",
  "short_description": "PPI network generation using GUILD",
  "long_description": "BioExcel Building Block tool to get in TXT the components of a PPI obtained using GUILD prioritization algo over Gene Variants associated to a disease, obtained from a DisGenet request",
  "url": "http://gitbub.com/mapoferri/Biobb_disgenet",
  "publication": "",
  "owner": {
    "author": "Maria Paola Ferri",
    "institution": "Barcelona Supercomputing Center",
    "contact": "maria.ferri@bsc.es",
    "url": ""
  },
  "status": 1,
  "external": true,
  "keywords": ["gene_variants", "data_retrieval"],
  "infrastructure": {
    "memory": 12,
    "cpus": 4,
    "container_image": "mapoferri/guild_tool:latest",
    "clouds": {
      "local": {
        "launcher": "docker_SGE",
	"default_cloud": true,
        "executable": "/home/vre_template_tool/VRE_RUNNER",
        "executable_type": "docker"
      }
    }
  },
  "input_files": [
    {
      "name": "yaml_config",
      "description": "BioBB workflow config file",
      "help": "Some explanation about 'My Input 1' displayed to user in help tooltips",
      "file_type": ["YAML"],
      "data_type": ["configuration_file"],
      "type": "hidden",
      "value": "biobb_disgenet/biobb-disc4alldata-setup.yaml",
      "required": true,
      "allow_multiple": false
    }
  ],
  "arguments": [
    {
      "name": "step1_gda_retrieval:diseaseid",
      "description": "Disease/s for which to find the associated genomic variants",
      "help": "Select one or more diseases",
      "type": "enum_multiple",
      "enum_items": {
        "name": ["C0410606", "C0263874", "C0263872", "C3494950", "C0158265"],
        "description": [
          "Cervical Disc Degenerative Disorder",
          "Lumbar disc Degenerative Disorder",
          "Thoracic Disc Degenerative Disorder",
          "Intervertebral disc dessication",
          "Degeneration of lumbar or lumbrosacral intervertebral disc"
        ]
      }
    },
    {
      "name": "step1_gda_retrieval:disease_id_other",
      "description": "Other disease ID",
      "help": "ID for the disease must follow UMLS CUI, and list of diseases separated by ',' up to 100.",
      "type": "string"
    },
    {
      "name": "step1_gda_retrieval:min_score",
      "description": "Min value of the gene-disease DisGeneT score range",
      "help": "To check how the score is computed, visit 'http://www.disgenet.org/dbinfo#section31'",
      "type": "string",
      "default": "0"
    },
    {
      "name": "step1_gda_retrieval:max_score",
      "description": "Max value of the gene-disease DisgeneT score",
      "help": "To check how the score is computed, visit 'http://www.disgenet.org/dbinfo#section31'",
      "type": "string",
      "default": "1"
    },
    {
      "name": "step2_parse_hippie:hippie_score",
      "description": "Define HIPPIE confidence value for the reliability of the interactions.",
      "help": "To check how the score is computed, visit 'http://cbdm-01.zdv.uni-mainz.de/~mschaefer/hippie/information.php#score'",
      "type": "string",
      "default": "0.2"
    },
    {
      "name": "step3_making_nodes:curated",
      "description": "Gene reference dataset should be curated or not.",
      "default": "False",
      "type": "boolean"
    },
    {
      "name": "step3_making_nodes:scoring_mode",
      "description": "Define the desired scoring coefficient to assign gene seeds relations.",
      "default": "dis",
      "type": "enum_multiple",
      "enum_items": {
        "name": ["dis", "norm_dis", "binary"],
        "description": ["DisGenet score", "Normalized DisGenet score", "Binary"]
      }
    },
    {
      "name": "step4_call_guild:algorithm",
      "description": "Define the principle algorithm used by GUILD for the prioritization of disease candidate genes.",
      "help": "To check how to choose the algorithm, visit 'http://sbi.imim.es/web/index.php/research/software/guildsoftware'",
      "default": "s",
      "type": "hidden",
      "value": "s"
    },
    {
      "name": "retrieve_by",
      "description": "dummy",
      "help": "dummy",
      "type": "hidden",
      "value": "Disease ID"
    }
  ],
  "has_custom_viewer": false,
  "output_files": [
    {
      "name": "output_netscore_path",
      "required": true,
      "allow_multiple": false,
      "file": {
        "file_type": "TXT",
        "data_type": "variants",
        "meta_data": {
          "visible": true,
          "description": "TXT file containing the variants/genomic ids correlated to the disease.",
          "tool": "biobb_disgenet"
        }
      }
    }
  ],
  "sites": [
    {
      "site_id": "local",
      "status": 1
    }
  ]
}
