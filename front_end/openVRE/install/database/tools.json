        "required": true,
        "allow_multiple": false,
        "value": "D4AUSER63f37e2112a69_6486ea0ebc6b41.64810731"
      },
      {
        "name": "vertical_vertebral_disc_coords",
        "description": "Initial coordinates of the vertebral disk for the vertical axis",
        "help": "Initial coordinates of the cellular system to be simulated",
        "file_type": ["OBJ"],
        "data_type": ["vertebral_disc_coordinates"],
        "required": true,
        "allow_multiple": false,
        "value": "D4AUSER63f37e2112a69_6486f41a8c0f78.07644683"
      }
    ],
    "input_files_public_dir": [],
    "input_files_combinations": [
      {
        "description": "Run 3D simulation",
        "input_files": ["vertical_vertebral_disc_coords", "horizontal_vertebral_disc_coords"]
      }
    ],
    "input_files_combinations_internal": [
      {
        "vertebral_disc_coordinates": "+"
      }
    ],
    "arguments": [
      {
        "name": "step1_iteration:device",
        "description": "Computational Resources available for the simulations.",
        "help": "Select one resource",
        "type": "enum_multiple",
        "enum_items": {
          "name": ["cpu"],
          "description": ["CPU"]
        }
      }
    ],
    "has_custom_viewer": false,
    "output_files": [
      {
        "name": "output_mesh_path",
        "required": true,
        "allow_multiple": false,
        "file": {
          "file_type": "PTH",
          "data_type": "3d_simulation",
          "meta_data": {
            "visible": true,
            "description": "Here goes a short description of what this output file contains",
            "tool": "biobb_23dmodel"
          }
        }
      }
    ],
    "sites": [
